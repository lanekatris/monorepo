<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <title>Masterchief</title>
<!--  <script src="https://cdn.tailwindcss.com"></script>-->
</head>
<body>

<div class="md:flex-grow mt-10">
  <h1 class="text-5xl font-medium text-gray-900 title-font mb-2 text-center">Session {{sessionId}}</h1>
</div>

<a href="/climb" class="underline text-blue-600 hover:text-blue-800 visited:text-purple-600 text-3xl">All Climbs</a>

{{#each model}}
<section class="text-gray-600 body-font overflow-hidden">
  <div class="container py-5 mx-auto">
    <div class="-my-8 divide-y-2 divide-gray-100">
      <div class="py-8 flex flex-wrap md:flex-nowrap items-center">
        <div class="md:w-64 md:mb-0 mb-6 flex-shrink-0 flex flex-col text-right">
          <form method="post" action="{{../canceledUrl}}">
            <input type="hidden" name="grade" value="{{this.grade}}"/>
            <input type="hidden" name="sessionId" value="{{../sessionId}}"/>
            <button type="submit" class="w-full rounded bg-blue-500 text-gray-100  focus:outline-none items-center py-7 " style="text-align:-webkit-center">
              <svg xmlns="http://www.w3.org/2000/svg" class="h-14 w-14" viewBox="0 0 20 20" fill="currentColor" style="text-align:-webkit-center">
                <path fill-rule="evenodd" d="M3 10a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1z" clip-rule="evenodd" />
              </svg>
            </button>
          </form>
        </div>
        <div class="md:flex-grow">
          <h1 class="text-7xl font-medium text-gray-900 title-font mb-2 text-center">{{this.grade}} ({{this.count}})</h1>
        </div>
        <div class="md:w-64 md:mb-0 mb-6 flex-shrink-0 flex flex-col">
          <form method="post" action="{{../completedUrl}}" class="flex">
            <input type="hidden" name="grade" value="{{this.grade}}"/>
            <input type="hidden" name="sessionId" value="{{../sessionId}}"/>
            <button type="submit" class="w-full rounded bg-blue-500 text-gray-100  focus:outline-none items-center py-7 " style="text-align:-webkit-center">
              <svg xmlns="http://www.w3.org/2000/svg" class=" flex h-14 w-14" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
              <path stroke-linecap="round" stroke-linejoin="round" d="M12 4v16m8-8H4" />
            </svg></button>
          </form>
        </div>
      </div>
    </div>
  </div>
</section>
{{/each}}

</body>
</html>
